Proceso conversion
	definir dolares,colones,conv Como Real;
	Escribir "Ingrese la cantidad de dolares";
	leer dolares;
	Escribir "¿Cuántos colones equivale un dolar?";
	leer colones;
	conv<-dolares*colones;
	Escribir ,dolares," ","dolares"," ","equivale a"," ",conv," ","colones";
	
FinProceso
